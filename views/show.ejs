<% include ./partials/header.ejs %>

<div class="bg-gray-900 px-7 py-2 text-white w-full min-h-screen">
    <div class="cards flex flex-wrap gap-2">
        <% user.pins.forEach(function(param){ %>
            <div class="card w-24">
                <div class="w-full h-32 rounded-lg overflow-hidden">
                    <img src="/images/uploads/<%= param.image %>" alt="" class=" w-full h-full object-center object-cover">
                </div>
                <h5 class="mt-2 text-sm font-semibold"><%= param.title %></h5>
            </div>
        <% }) %>
    </div>
</div>

<% include ./partials/footer.ejs %>
